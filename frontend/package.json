{
  "name": "coderscassonne-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"tsc -w\" \"vite\"",
    "build": "tsc && vite build --base=./",
    "build:tailwind": "tailwindcss build src/index.css -o src/tailwind.output.css",
    "start": "vite preview",
    "eject": "react-scripts eject",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage ",
    "test:ci": "jest --ci --silent --coverage",
    "format": "prettier --write \"src/**/*.{jsx,js,scss,css,md,json}\"",
    "lint": "eslint . **/*.ts"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "flowbite": "^1.4.5",
    "lodash": "^4.17.21",
    "mobx": "^6.5.0",
    "mobx-react": "^7.3.0",
    "mobx-react-lite": "^3.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-player": "^2.10.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.0",
    "socket.io-client": "^4.5.0",
    "web-vitals": "^2.1.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.181",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-router-dom": "^5.3.3",
    "@types/testing-library__jest-dom": "^5.14.3",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.1.0",
    "eslint": "^8.12.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-airbnb-typescript": "^16.1.4",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "^2.25.0",
    "eslint-plugin-jest": "24.4.2",
    "eslint-plugin-jest-dom": "^3.9.2",
    "eslint-plugin-jest-formatting": "3.0.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "^7.25.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-testing-library": "^5.0.0",
    "postcss": "^8.4.12",
    "tailwindcss": "^3.0.23",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "vite": "^2.8.6",
    "vite-react-jsx": "^1.1.2"
  }
}
